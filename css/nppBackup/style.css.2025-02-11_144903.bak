@media only screen and (max-width: 768px) {
  /* 두번째 섹션에 한해 행을 flex 컨테이너로 만듭니다 */
  .research-2 .row {
    display: flex;
    flex-direction: column;
  }
  /* 기본 HTML 순서가 텍스트 → 이미지라면, 
     이미지 칼럼을 order: -1 로 지정하여 위로 올립니다 */
  .research-2 .row > .col-md-6:last-child {
    order: 0;
  }
  /* (선택사항) 텍스트 칼럼의 order를 0으로 설정 */
  .research-2 .row > .col-md-6:first-child {
    order: -1;
  }
}


